{
  "version": 2,
  "buildCommand": "ng build",
  "outputDirectory": "dist",
  "framework": "angular",
  "routes": [
    {
      "src": "/absence/justification/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}