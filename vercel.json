{
  "version": 2,
  "framework": "angular",
  "buildCommand": "ng build --configuration production",
  "outputDirectory": "dist/gestion-absence-angular",
  "routes": [
    {
      "src": "/absence/justification/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "prerender": {
    "enabled": false
  }
}